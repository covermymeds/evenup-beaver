[<%= @source_real %>]
<% if @redis_url -%>
redis_url: <%= @redis_url %>
<% end -%>
<% if @redis_namespace -%>
redis_namespace: <%= @redis_namespace %>
<% end -%>
type: <%= @type %>
<% if @tags.is_a?(Array) -%>
tags: <%= @tags.join(',') %>
<% elsif @tags.is_a?(String) -%>
tags: <%= @tags %>
<% end -%>
<% if @format -%>
format: <%= @format %>
<% end -%>
<% if scope.lookupvar('beaver::enable_sincedb') -%>
sincedb_write_interval: <%= scope.lookupvar('sincedb_write_interval') %>
<% end -%>
<% unless @exclude.empty? -%>
<% if @exclude.is_a?(Array) -%>
exclude: (<%= @exclude.join('|') %>)
<% elsif @exclude.is_a?(String) -%>
exclude: (<%= @exclude %>)
<% elsif @add_field.is_a?(String) -%>
add_field: (<%= @add_field %>)
<% elsif @multiline_regex_before.is_a?(String) -%>
multiline_regex_before: (<%= @multiline_regex_before %>)
<% elsif @multiline_regex_after.is_a?(String) -%>
multiline_regex_after: (<%= @multiline_regex_after %>)
<% end -%>
<% end -%>

